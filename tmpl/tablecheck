#if COCOS2D_DEBUG >= 1 
  if(!tolua_isusertable(tolua_S, 1, "${Space}.${Class}", 0, &tolua_err)){
    goto tolua_lerror;
  }
#endif

  cobj = (${Class}*)tolua_tousertype(tolua_S, 1, 0);

#if COCOS2D_DEBUG >= 1 
  if (!cobj) {
    tolua_error(tolua_S, "invalid 'cobj' in function 'lua_${Class}_${Method}'.");
    return 0;
  }
#endif

